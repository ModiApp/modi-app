{
  "cli": {
    "version": ">= 16.17.3",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_CONNECT_TO_PROD": "1"
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_CONNECT_TO_PROD": "1"
      }
    },
    "simulator": {
      "ios": {
        "simulator": true
      },
      "env": {
        "EXPO_PUBLIC_CONNECT_TO_PROD": "1"
      }
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "EXPO_PUBLIC_CONNECT_TO_PROD": "1"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "ascAppId": "1498350892"
      }
    }
  }
}
