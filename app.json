{
  "expo": {
    "name": "modi-again",
    "slug": "modi-again",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./ui/assets/images/icon.png",
    "scheme": "modi",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "owner": "ikeybenz",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ikeybenz.modi",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./ui/assets/images/adaptive-icon.png",
        "backgroundColor": "#35654D"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.ikeybenz.modi"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./ui/assets/images/favicon.png",
      "build": {
        "babel": {
          "include": [
            "@expo/vector-icons"
          ]
        }
      }
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#35654D"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "1c48f7a3-d55e-4a9c-b077-1e421bbd5906"
      }
    }
  }
}
